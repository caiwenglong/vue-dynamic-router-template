{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "useBuiltIns": "entry", //IE9兼容
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
        }
      }
    ],
    "stage-2"
  ],
  "env": {
    "browse": {
      "plugins": ["istanbul"]
    },
    "test": {
      "plugins": ["istanbul"]
    }
  },
  "plugins": ["transform-vue-jsx", "transform-runtime"]
}
